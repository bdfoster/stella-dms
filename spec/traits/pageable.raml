#%RAML 1.0 Trait
queryParameters:
  offset:
    description: The number of <<resourcePath>> to skip.
    type: number
    required: false
    minimum: 0
    example: 0
  limit:
    description: The number of <<resourcePath>> to return.
    type: number
    required: false
    minimum: 1
    maximum: 100
    example: 20
    default: 20